 二分查找
#include<stdio.h>
#include<string.h>
int binary_search(int arr[], int k,int sz) 
{
	int left = 0;
	int right = sz - 1;
	while (left <= right)
	{
		int mid = (left + right) / 2;
		if (arr[mid] > k)
		{
			right = mid - 1;
		}
		else  if (arr[mid] < k)
		{
			left = mid + 1;
		}
		else {
			return mid;
		}
	
	}
		return -1;
}

int main()
{
	int arr[] = { 1,2,3,4,6,7,8,10 };
	int k = 8;
	int sz = sizeof(arr)/sizeof(arr[0]) - 1;
						//arr传递的是首元素的地址
	int ret=binary_search(arr, k,sz);
	if (-1 == ret) 
	{
		printf("找不到！");
	}
	else
		printf("找到了，下标是：%d\n", ret);
	return 0;
} 

#include <stdio.h>
int main()
{
    char s[11];                             //定义一个长度为10的字符串（注意有结尾符）
    gets(s);                                //输入字符串
    for (int i = 0; s[i] != '\0'; i++)      //循环到结尾符'\0'时停止循环
    {
        if (s[i] >= 'A' && s[i] <= 'Z')             //判断是否为大写字母
        {
            s[i] = (s[i] + 4 - 'A') % 26 + 'A';     //大写字母后移
        }
        if (s[i] >= 'a' && s[i] <= 'z')             //判断是否为小写字母A
        {
            s[i] = (s[i] + 4 - 'a') % 26 + 'a';     //小写字母后移
        }
    }
    puts(s);                                //输出字符串
    return 0;
}
递归
#include<stdio.h>
void print(int n) {
	if (n > 9) {
		print(n / 10);
	}
	printf("%d ", n % 10);

}
int main()
{
	int s = 0;
	scanf("%d", &s);
	print(s);
	return 0;
}


#include<stdio.h>
int main()
{
	int n,Sn = 0;
	int a=0,i = 0;
	scanf("%d", &n);
	for (i = 1; i <= n; i++) {
		
		a = a * 10 + 2;
		Sn = Sn + a;
		
	}
	printf("%d", Sn);
	return 0;
}

