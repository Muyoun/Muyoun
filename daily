#include<stdio.h>
void print(int n) {
	int i = 0;
	for (i = 1; i <= n; i++) {
		printf("%d\n", i);
	}
}
int main()
{
	int N = 0;
	scanf("%d", &N);
	 print(N);
	return 0;
}
计算利润
#include<stdio.h>
int main()
{
	 long long int n= 0;
	int s = 0;
	printf("请输入当月利润：");
	scanf("%lld", &n);
	if (0<n <= 100000) 
		s = n * 0.1;
	
	else if (n <= 200000) 
		s = 100000 * 0.1 + (n - 100000) * 0.075;
	
	else if (n <= 400000) 
		s = 100000 * 0.175 + (n - 200000) * 0.05;
	
	else if (n <= 600000) 
		s = 100000 * 0.175 + 200000 * 0.05 + (n - 400000) * 0.03;
	
	else if (n <= 1000000) 
		s = 100000 * 0.175 + 200000 * 0.08 + (n - 600000) * 0.015;
	
	else 
		s = 100000 * 0.175 + 200000 * 0.08 + 400000 * 0.015+(n-1000000)* 0.01;
	printf("s=%d\n", s);
	return 0;
}
